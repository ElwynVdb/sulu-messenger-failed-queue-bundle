{
    "name": "tailr/sulu-messenger-failed-queue-bundle",
    "type": "symfony-bundle",
    "autoload": {
        "psr-4": {
            "Tailr\\SuluMessengerFailedQueueBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tailr\\SuluMessengerFailedQueueBundle\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.1",
        "azjezz/psl": "^2.6",
        "doctrine/orm": "^2.14",
        "sulu/sulu": "^2.5",
        "jackalope/jackalope-doctrine-dbal": "^1.7",
        "symfony/config": "^6.0",
        "symfony/console": "^6.0",
        "symfony/dependency-injection": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/doctrine-messenger": "^6.0",
        "symfony/messenger": "^6.0"
    },
    "require-dev": {
        "phpro/symfony-conventions": "^1.4",
        "psalm/plugin-symfony": "^5.0",
        "vimeo/psalm": "^5.13",
        "weirdan/doctrine-psalm-plugin": "^2.8"
    },
    "config": {
        "allow-plugins": {
            "phpro/grumphp-shim": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        },
        "sort-packages": true
    }
}
